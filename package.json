{
  "name": "clean-architecture-with-riverpod",
  "displayName": "Clean Architecture with Riverpod",
  "description": "Flutter 프로젝트를 위한 간단한 Clean Architecture 및 Riverpod 구조 자동 생성 확장",
  "version": "1.0.3",
  "icon": "images/icon.png",
  "publisher": "biso",
  "repository": {
    "type": "git",
    "url": "https://github.com/bisor0627/vs-ext-clean-architecture-with-riverpod.git"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Flutter",
    "Riverpod",
    "CleanArchitecture",
    "Folder Structure",
    "Scaffolding",
    "Boilerplate"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createFolderTree",
        "title": "🗂️ Create: Feature Folder Structure",
        "category": "Clean Architecture with Riverpod"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createFolderTree",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  }
}
